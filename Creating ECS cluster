InstanceId = sh(script: "aws ec2 describe-instances --filters Name=tag:Name,Values=$EC2_Instance --query \"Reservations[*].Instances[*].InstanceId\" --output text", returnStdout:true).trim()
def instance_status = sh(script: "aws ec2 describe-instances --filters Name=tag:Name,Values=$EC2_Instance Name=instance-state-name,Values=running,stopped | jq '.Reservations[0].Instances[0].State.Name' | tr -d '\"'", returnStdout:true).trim()
